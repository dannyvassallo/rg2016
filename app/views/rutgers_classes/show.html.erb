<%= @class.class_name %>
<%= link_to "new student", new_rutgers_class_student_path(:rutgers_class_id => @class.friendly_id) %>
<br>
<% @class.students.each do |student| %>
  <%= link_to student.name, rutgers_class_student_path(@class, student) %><br>
<% end %>
<br>
<%= form_tag import_rutgers_class_students_path(@class), multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import Csv" %>
<% end %>
